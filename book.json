        "Home": "https://azraeil.gitbooks.io/my-note/content/"
    }
  },
  "plugins": [
    "ga",
    "-lunr",
    "-search",
    "-highlight",
    "-livereload",
    "search-plus@^0.0.11",
    "simple-page-toc@^0.1.1",
    "github@^2.0.0",
    "github-buttons@2.1.0",
    "edit-link@^2.0.2",
    "disqus@^0.1.0",
    "advanced-emoji@^0.2.1",

    "include-codeblock@^3.0.2",
    "ace@^0.3.2",
    "emphasize@^1.1.0",
    "katex@^1.1.3",
    "splitter@^0.0.8",
    "mermaid-gb3@2.1.0",
    "tbfed-pagefooter@^0.0.1",
    "local-video@^1.0.1",
    "favicon@^0.0.2",
    "todo@^0.1.3",
    "alerts@^0.2.0",
    "include-csv@^0.1.0",
